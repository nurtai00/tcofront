{
  "name": "frontend-template",
  "version": "1.0.0",
  "private": true,
  "contributors": [
    {
      "name": "Aizhan Tolebayeva",
      "email": "hasnochil@gmail.com",
      "url": "https://github.com/aizhantol"
    },
    {
      "name": "Alizhan Abdul Khalil",
      "email": "alizhan.abdulkhalil@gmail.com",
      "url": "https://github.com/aleezhan"
    }
  ],
  "scripts": {
    "dev": "nuxt --open",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install",
    "test": "jest",
    "create-component": "node index.cli.js component",
    "create-page": "node index.cli.js page",
    "create-store": "node index.cli.js store",
    "svg-font": "yarn add svgicons2svgfont svg2ttf && node bin/svg2ttf && svgicons2svgfont --fontname=iconsfont --prependUnicode --preserveAspectRatio --height=1024 --normalize assets/icons/*.svg > assets/icons/formatted/iconsfont.svg && svg2ttf assets/icons/formatted/iconsfont.svg assets/icons/formatted/iconsfont.ttf && yarn remove svgicons2svgfont svg2ttf"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint, prettier",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/auth-next": "^5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "core-js": "^3.19.3",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "5.5.0",
    "jsona": "^1.9.7",
    "mini-css-extract-plugin": "2.6.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.8",
    "sass": "^1.52.3",
    "sass-loader": "10.1.1",
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^12.0.0",
    "vue": "^2.6.14",
    "vue-apexcharts": "^1.6.2",
    "vue-client-only": "^2.1.0",
    "vue-select": "^3.20.0",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "vue-jest": "^3.0.4"
  }
}
